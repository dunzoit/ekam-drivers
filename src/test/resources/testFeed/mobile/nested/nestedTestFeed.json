{
  "targets": [
    {
      "app": "calculator.apk",
      "platform": "android",
      "desiredCapabilities": {
        "newCommandTimeout": 300,
        "autoGrantPermissions": true,
        "noReset": true,
        "avdLaunchTimeout": 300,
        "autoAcceptAlerts": true,
        "useKeystore": false
      }
    },
    {
      "app": "calculator.apk",
      "platform": "iOS",
      "desiredCapabilities": {
        "newCommandTimeout": 300,
        "autoGrantPermissions": true,
        "noReset": true,
        "avdLaunchTimeout": 300,
        "autoAcceptAlerts": true,
        "useKeystore": false
      }
    }
  ],
  "serverArguments": [
    "--enable-console-logs",
    "--log-level info"
  ],
  "executables": {
    "appiumJSPath": "",
    "nodePath": ""
  },
  "deviceFilters": {
    "platformVersion": {
      "include": [
        "10.0",
        "14.4"
      ],
      "operator": "=",
      "exclude": []
    },
    "model": {
      "include": [
        "OnePlus 7",
        "Xiaomi Redmi Note 9"
      ],
      "exclude": []
    },
    "udid": {
      "include": [
      ],
      "exclude": [
      ]
    }
  }
}
