  "deviceFilters": {
    "platformVersion": {
      "include": [
        "7.1.1"
      ],
      "operator": "=",
      "exclude": []
    },
    "model": {
      "include": [
        "",
        ""
      ],
      "exclude": []
    },
    "udid": {
      "include": [
      ],
      "exclude": [
      ]
    }
  }
}
