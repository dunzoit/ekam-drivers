{
  "app": "calculator.apk",
  "platform": "android",
  "desiredCapabilities": {
    "newCommandTimeout": 300,
    "autoGrantPermissions": true,
    "noReset": true,
    "avdLaunchTimeout": 300,
    "autoAcceptAlerts": true,
    "useKeystore": false,
    "customCap": "customValue"
